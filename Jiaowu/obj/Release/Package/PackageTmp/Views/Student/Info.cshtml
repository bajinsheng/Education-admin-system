@{
    Layout = null;
}
<!DOCTYPE HTML PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <title>学生自主管理系统</title>
    <meta name="keywords" content="index" />
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" />
    <meta name="renderer" content="webkit" />
    <meta http-equiv="Cache-Control" content="no-siteapp" />
    <link rel="icon" type="image/png" href="~/assets/i/favicon.png" />
    <link rel="apple-touch-icon-precomposed" href="~/assets/i/app-icon72x72@2x.png" />
    <meta name="apple-mobile-web-app-title" content="Amaze UI" />
    <link href="~/assets/css/amazeui.min.css" rel="stylesheet" />
    <link rel="stylesheet" href="~/assets/css/admin.css" />
</head>
<body>

    <div>
        <div class="am-cf am-padding">
            <div class="am-fl am-cf"><strong class="am-text-primary am-text-lg">个人信息</strong> / <small>Personal information</small></div>
        </div>
        <hr />
        <div class="am-g">          
            <div >
                <div >
                    <div class="am-form-group">
                        <div class="am-u-sm-9">
                            <p>姓名 ：@ViewData["Name"]</p>
                        </div>
                    </div>
                    <div class="am-form-group">
                        <div class="am-u-sm-9">
                            <p>学号 ：@ViewData["Sno"]</p>
                        </div>
                    </div>
                    <div class="am-form-group">
                        <div class="am-u-sm-9">
                            <p>性别 ：@ViewData["Sex"]</p>
                        </div>
                    </div>
                    <div class="am-form-group">
                        <div class="am-u-sm-9">
                            <p>专业 ：@ViewData["Pname"]</p>
                        </div>
                    </div>
                    <div class="am-form-group">
                        <div class="am-u-sm-9">
                            <p>班级 ：@ViewData["Classname"]</p>
                        </div>
                    </div>
                    <div class="am-form-group">
                        <div class="am-u-sm-9">
                            <p>生日 ：@ViewData["Bir"]</p>
                        </div>
                    </div>                               
                </div>
            </div>
        </div>
    </div>




</body>
</html>
